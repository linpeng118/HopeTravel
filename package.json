{
  "name": "nuxt",
  "version": "1.0.0",
  "description": "My best Nuxt.js project",
  "author": "c_illusion",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ENV_TYPE=prodDev nodemon server/index.js --watch server --exec babel-node",
    "build": "cross-env NODE_ENV=production ENV_TYPE=prodDev nuxt build",
    "build-qa": "cross-env NODE_ENV=production ENV_TYPE=prodQa nuxt build",
    "build-pre": "cross-env NODE_ENV=production ENV_TYPE=prodPre nuxt build",
    "build-master": "cross-env NODE_ENV=production ENV_TYPE=prodMaster nuxt build",
    "start": "cross-env NODE_ENV=production ENV_TYPE=prodDev node server/index.js",
    "start-qa": "cross-env NODE_ENV=production ENV_TYPE=prodQa node server/index.js",
    "start-pre": "cross-env NODE_ENV=production ENV_TYPE=prodPre node server/index.js",
    "start-master": "cross-env NODE_ENV=production ENV_TYPE=prodMaster node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.0.0",
    "axios": "0.18.0",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "cross-env": "5.2.0",
    "koa": "2.5.2",
    "lodash": "4.17.11",
    "node-sass": "4.11.0",
    "nprogress": "^0.2.0",
    "nuxt": "2.0.0",
    "postcss-px2rem-exclude": "0.0.6",
    "qs": "6.6.0",
    "sass-loader": "7.1.0",
    "vant": "1.5.1",
    "vue-awesome-swiper": "3.1.3",
    "vue-clipboard2": "0.2.1",
    "vue-cropper": "0.4.8",
    "vue-lazyload": "1.2.6",
    "vuex-persist": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/babel-preset-app": "^2.4.5",
    "@nuxtjs/proxy": "1.3.1",
    "autoprefixer": "9.4.4",
    "babel-eslint": "8.2.1",
    "babel-loader": "^8.0.5",
    "eslint": "5.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-vue": "4.0.0",
    "nodemon": "1.11.0",
    "vconsole": "3.2.0"
  }
}
